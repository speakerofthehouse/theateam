<h2>Your Playlists:</h2>
<div id="scroll-div">
<ul id="playlists" >
  <% playlists.forEach(function(playlist){ %>
    <li>
      <button class="playlist" id="<%= playlist.id %>" name="<%= playlist.id %>" onclick="renderTracklist(this)">
        <%= playlist.name %>
      </button>
    </li>
  <% }); %>
</ul>
</div>
